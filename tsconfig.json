{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "~/*": [ "./src/*" ]
    },
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
